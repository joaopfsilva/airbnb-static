<h1 class="text-center padding">Our team</h1>

<ul class="list-inline text-center">
  <% ["anne", "seb", "romain"].each do |member| %>
    <li>
      <h3><%= member.capitalize %></h3>
      <%= image_tag "#{member}.jpg", class: "avatar-extra-large" %>
    </li>
  <% end %>
</ul>

<% link_to "#" do %>
    <h2 class="padded text-center"><i class="fa fa-envelope"></i>
    contact@airbnb.com</h2>
<% end %>
